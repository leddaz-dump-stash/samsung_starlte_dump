<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.app.launcher">
    <homeGridInfo default="5x5"/>

    <home>
        <!-- appwidget : Weather -->
        <appwidget 
            screen="0" 
            packageName="com.sec.android.daemonapp" 
            className="com.sec.android.daemonapp.appwidget.WeatherAppWidget2x1" 
            x="1" 
            y="0" 
            spanX="3" 
            spanY="3" />

        <!-- appwidget : Google App -->
        <appwidget 
            screen="0" 
            packageName="com.google.android.googlequicksearchbox" 
            className="com.google.android.googlequicksearchbox.SearchWidgetProvider" 
            x="0" 
            y="4" 
            spanX="5" 
            spanY="1" />

        <!-- appwidget : My Optus -->
        <appwidget 
            screen="1" 
            packageName="au.com.optus.selfservice" 
            className="au.com.optus.selfservice.ui.widget.MyOptusWidgetProvider" 
            postPosition="true"
            x="0" 
            y="0" 
            spanX="5" 
            spanY="1" />

        <!-- My Optus -->
        <favorite 
            screen="1" 
            packageName="au.com.optus.selfservice" 
            className="au.com.optus.express.moa.onboarding.LauncherActivity" 
            x="0" 
            y="4" />

        <!-- Gallery -->
        <favorite 
            screen="1" 
            packageName="com.sec.android.gallery3d" 
            className="com.samsung.android.gallery.app.activity.GalleryActivity" 
            x="1" 
            y="4" />

        <!-- Chrome -->
        <favorite 
            screen="1" 
            packageName="com.android.chrome" 
            className="com.google.android.apps.chrome.Main" 
            x="2" 
            y="4" />

        <!-- Calendar -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.calendar" 
            className="com.samsung.android.app.calendar.activity.MainActivity" 
            x="3" 
            y="4" />

        <!-- Email -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.email.provider" 
            className="com.samsung.android.email.ui.activity.MessageListXL" 
            x="4" 
            y="4" />

    </home>


    <hotseat>
        <!-- Phone -->
        <favorite 
            screen="0" 
            packageName="com.samsung.android.dialer" 
            className="com.samsung.android.dialer.DialtactsActivity" />

        <!-- Messages -->
        <favorite 
            screen="1" 
            packageName="com.samsung.android.messaging" 
            className="com.android.mms.ui.ConversationComposer" />

        <!-- Samsung Internet -->
        <favorite 
            screen="2" 
            packageName="com.sec.android.app.sbrowser" 
            className="com.sec.android.app.sbrowser.SBrowserMainActivity" />

        <!-- Google Play Store -->
        <favorite 
            screen="3" 
            packageName="com.android.vending" 
            className="com.android.vending.AssetBrowserActivity" />

        <!-- Camera -->
        <favorite 
            screen="4" 
            packageName="com.sec.android.app.camera" 
            className="com.sec.android.app.camera.Camera" />

    </hotseat>

</favorites>